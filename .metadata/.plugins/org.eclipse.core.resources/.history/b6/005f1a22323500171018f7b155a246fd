package acquireResource;


public class AcquireDiskIO {
public static String CMD="iostat -d -x 1 2";

public static void getDiskIORate(String info){
	String diskName=null;
	String rkb=null;
	String wkb=null;
	String[] data=info.split("\n");
	for (int i = 7; i < data.length; i++) {
		String[] numdata=data[i].split(" +");
		diskName=numdata[0];
		rkb=numdata[5];
		wkb=numdata[6];
		System.out.println("diskname:"+diskName+" rkb:"+rkb+"kB/s wkb:"+wkb+"kB/s");
	}
}
public static void main(String[] args) {
	String info=Runcommand.runCommand(CMD); 
	System.out.println(info);
    getDiskIORate(info);
}

}
